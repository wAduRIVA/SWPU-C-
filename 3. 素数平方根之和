#include<stdio.h>
#include<math.h>
int main()
{
        int i,j;
        double sum=0.0;
/**********Program**********/
        for(i=3;i&lt;101;i++)
		{
			for(j=2;j&lt;=i;j++)
				if(i%j==0)
				  break;
			    if(j==i)
				sum=sum+sqrt((double)i);		 
		}
/**********  End  **********/
        printf("%lf\n",sum);
        return 0;
}